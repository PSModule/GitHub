{
    "filters": {
        "comments": true
    },
    "rules": {
        "spelling": {
            "language": "en",
            "skipPatterns": [
                "JavaScript"
            ],
            "wordDefinitionRegexp": "/[\\w']+/g",
            "suggestCorrections": true
        },
        "abbr-within-parentheses": true,
        "rousseau": {
            "showLevels": [
                "suggestion",
                "warning",
                "error"
            ]
        },
        "common-misspellings": {
            "ignore": []
        }
    }
}
